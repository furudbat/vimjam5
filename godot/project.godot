config_version=5

[application]

config/name="Echoes of the Aztec Beast"
config/description="Entry for VimJam5 - Run from the Aztec Beast Ahuizotl by solving Microgames and escape the temple"
config/version="0.1.0"
run/main_scene="res://scenes/main.tscn"
config/features=PackedStringArray("4.3")
run/max_fps=60
boot_splash/bg_color=Color(0.188235, 0.129412, 0.176471, 1)
boot_splash/show_image=false
boot_splash/fullsize=false
boot_splash/use_filter=false
config/icon="res://assets/icon/128x128.png"

[autoload]

ColorPalette="*res://autoloads/color_palette.gd"
Constants="*res://autoloads/constants.gd"
ThemeManager="*res://autoloads/theme_manager.gd"
UserSettings="*res://autoloads/user_settings.gd"
SoundManager="*res://addons/sound_manager/sound_manager.gd"
GlobalScenes="*res://autoloads/scenes.gd"
MiniGames="*res://autoloads/mini_games.gd"
PhantomCameraManager="*res://addons/phantom_camera/scripts/managers/phantom_camera_manager.gd"
Signal_Debugger="*res://addons/SignalVisualizer/Debugger/SignalDebugger.gd"

[display]

window/size/viewport_width=426
window/size/viewport_height=240
window/size/resizable=false
window/size/window_width_override=854
window/size/window_height_override=480
window/stretch/mode="canvas_items"
window/stretch/scale_mode="integer"

[editor_plugins]

enabled=PackedStringArray("res://addons/ColorPreview/plugin.cfg", "res://addons/SignalVisualizer/plugin.cfg", "res://addons/donut_collision_polygon2D/plugin.cfg", "res://addons/phantom_camera/plugin.cfg", "res://addons/sound_manager/plugin.cfg")

